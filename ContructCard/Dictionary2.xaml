<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="TargetTextBloc" TargetType="TextBlock">
        <Setter Property="Visibility" Value="Visible"/>
    </Style>
    <Style x:Key="TargetTextBlocVisible" TargetType="TextBlock">
        <Setter Property="Visibility" Value="Visible"/>
    </Style>
    <Style x:Key="LabelVisible" TargetType="Label">
        <Setter Property="Visibility" Value="Visible"/>
    </Style>
    <Style x:Key="LabelText" TargetType="Label">
        <Setter Property="Visibility" Value="Visible"/>
    </Style>
    <Style x:Key="TextBoxSettings" TargetType="TextBox">
        <Setter Property="Canvas.Top" Value="203"/>
        <Setter Property="Height" Value="47"/>
    </Style>
    <Style x:Key="RectangleSettings" TargetType="Rectangle">
        <Setter Property="Canvas.Top" Value="39"/>
        <Setter Property="Height" Value="155"/>
    </Style>
    <Style x:Key="ImageVisible" TargetType="Image">
        <Setter Property="Visibility" Value="Visible"/>
    </Style>
</ResourceDictionary>